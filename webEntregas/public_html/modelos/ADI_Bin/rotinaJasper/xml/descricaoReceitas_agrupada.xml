	<queryString><![CDATA[SELECT BFFRP.seqReceita, SUM( BFFRP.valReceitaParcela ) AS valorTotalDaReceita, 
    BR.siglaDaReceita, BR.descrReduzida, basTR.descrTipoREceita, 
    BR.classificContabil as codDaContaReceita, 6 AS codSistema

FROM basFFReceitasParcela BFFRP

INNER JOIN basReceitas BR ON
    BFFRP.seqReceita = BR.seqReceita

INNER JOIN basTipoDeReceita basTR ON 
    BR.seqTipoReceita = basTR.seqTipoReceita

WHERE BFFRP.codsistema = $P!{codSistema}
    AND BFFRP.seqControlCalc = $P!{seqControlCalc}
    AND BFFRP.seqFFParcela = $P!{seqFFParcela}
    AND BFFRP.seqExercicio = $P!{seqExercicio}
    AND BFFRP.valReceitaParcela > 0

GROUP BY BFFRP.seqReceita, BR.siglaDaReceita, BR.descrReduzida, 
    basTR.descrTipoREceita, BR.classificContabil, 6

ORDER BY codDaContaReceita asc
]]></queryString>

